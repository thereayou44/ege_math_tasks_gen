            "https://ege.sdamgia.ru/formula/svg/21/21f6a8eb1e0ad73fea12a6517c98ede3.svg",
            "https://ege.sdamgia.ru/formula/svg/ea/ead55b55535c59c2dd12e8ed3856fb4c.svg",
            "https://ege.sdamgia.ru/formula/svg/f3/f37fe9d150dba482f14a997cdfd9e8ad.svg",
