            "https://ege.sdamgia.ru/formula/svg/f5/f5b039033ff0f3e779cea9e58757f1d7.svg",
            "https://ege.sdamgia.ru/formula/svg/f5/f5b039033ff0f3e779cea9e58757f1d7.svg",
            "/get_file?id=111635"
